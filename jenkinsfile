pipeline {
    agent any

    stages {
        stage('Checkout branch') {
            steps {
                sh "git clone "https://github.com/aliya1928/project.git""
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
